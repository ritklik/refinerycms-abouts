<%= render 'refinery/admin/search_header', :url => refinery.abouts_admin_abouts_path %>
<div class='pagination_container'>
  <% if @abouts.any? %>
    <%= render 'abouts' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
